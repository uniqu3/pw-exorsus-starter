{
    "private": true,
    "name": "exorsus",
    "description": "Default Processwire installation with standard layout and settings",
    "version": "1.0.0",
    "config": {
        "path_public": "./public",
        "path_assets": "./public/site/templates/assets",
        "hostname": "localhost"
    },
    "scripts": {
        "clean": "rimraf $npm_package_config_path_assets",
        "start": "webpack-dev-server --mode development --inline --hot --progress --config ./config/webpack.development.js",
        "watch": "webpack --mode development --watch --progress --config ./config/webpack.development.js",
        "build": "npm run build:dll -- --mode production && webpack --mode production --progress --config ./config/webpack.production.js",
        "build:dev": "npm  run build:dll -- --mode development && webpack --mode development --progress --config ./config/webpack.development.js",
        "build:dll": "webpack --display-error-details --progress --config ./config/webpack.dll.js"
    },
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/plugin-transform-object-assign": "^7.2.0",
        "@babel/polyfill": "^7.6.0",
        "@babel/preset-env": "^7.6.3",
        "@babel/preset-react": "^7.6.3",
        "@babel/preset-stage-1": "^7.0.0",
        "@babel/register": "^7.6.2",
        "autoprefixer": "^9.6.4",
        "babel-loader": "^8.0.6",
        "babel-plugin-add-module-exports": "^0.2.1",
        "bundled-dependencies": "^2.1.0",
        "copy-webpack-plugin": "^5.0.4",
        "core-js": "^3.2.1",
        "cross-env": "^6.0.3",
        "css-hot-loader": "^1.4.4",
        "css-loader": "^3.2.0",
        "expose-loader": "^0.7.5",
        "file-loader": "^4.2.0",
        "imports-loader": "^0.8.0",
        "install": "^0.13.0",
        "less": "^3.10.3",
        "less-loader": "^5.0.0",
        "mini-css-extract-plugin": "^0.8.0",
        "npm": "^6.12.0",
        "optimize-css-assets-webpack-plugin": "^5.0.3",
        "postcss": "^7.0.18",
        "postcss-import": "^12.0.1",
        "postcss-loader": "^3.0.0",
        "style-loader": "^1.0.0",
        "terser-webpack-plugin": "^2.1.3",
        "webpack": "^4.41.1",
        "webpack-cli": "^3.3.9",
        "webpack-dev-server": "^3.8.2",
        "webpack-merge": "^4.2.2"
    },
    "dependencies": {
        "aos": "^2.3.4",
        "exorsus.css": "^1.0.0",
        "history": "^4.10.1",
        "is_js": "^0.9.0",
        "leaflet": "^1.6.0",
        "lg-zoom.js": "^1.0.1",
        "lightgallery.js": "^1.1.3",
        "load-google-maps-api": "^1.3.3",
        "lodash.debounce": "^4.0.8",
        "normalize.css": "^8.0.1",
        "swiper": "^5.0.4"
    }
}
